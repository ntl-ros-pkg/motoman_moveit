<launch>
  <include file="$(find motoman_sia5_moveit_config)/launch/moveit_planning_execution.launch">
	<arg name="load_robot_description" value="true"/>
	<arg name="urdf_model" value="$(find motoman_description)/robots/sia5/urdf/sia5_with_jamming_and_kinectv2.urdf.xacro"/>
	<arg name="srdf_model" value="$(find motoman_moveit)/config/sia5_nishida_lab/sia5.srdf"/>
	<arg name="joint_limits_config" value="$(find motoman_moveit)/config/sia5_nishida_lab/joint_limits.yaml"/>
	<arg name="kinematics_config" value="$(find motoman_moveit)/config/sia5_nishida_lab/kinematics.yaml"/>
	<arg name="controllers_config" value="$(find motoman_moveit)/config/sia5_nishida_lab/controllers.yaml"/>
	<arg name="use_depth_sensor" value="true"/>
	<arg name="sensor_config" value="$(find motoman_moveit)/config/sia5_nishida_lab/kinect2.yaml"/>
	<arg name="octomap_frame_name" value="kinect2_link"/>
	<arg name="sensor_max_range" value="4.5"/>
	<arg name="rviz_config" value="$(find motoman_moveit)/launch/rviz/moveit_sia5_nishida_lab.rviz"/>
  </include>
</launch>
